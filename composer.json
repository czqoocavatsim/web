{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-dompdf": "^2.0",
        "bugsnag/bugsnag-laravel": "^2.0",
        "erusev/parsedown": "^1.7",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.5",
        "laravel-notification-channels/discord": "^1.1",
        "laravel/framework": "^9.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^4.0",
        "laravelcollective/html": "^6.4",
        "lasserafn/php-initial-avatar-generator": "^4.0",
        "lasserafn/php-initials": "^3.1",
        "league/flysystem-aws-s3-v3": "^3.1",
        "livewire/livewire": "^2.3",
        "marvinlabs/laravel-discord-logger": "^1.4",
        "misterphilip/maintenance-mode": "~2.0",
        "mofodojodino/profanity-filter": "^1.3",
        "sentry/sentry-laravel": "^3.4",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-cookie-consent": "^3.2",
        "spatie/laravel-html": "^3.2",
        "spatie/laravel-permission": "^5.10"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.13",
        "spatie/laravel-ignition": "^1.6.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^6.0",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
